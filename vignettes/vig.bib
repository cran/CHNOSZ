@ARTICLE{BBA+03,
  author = {Boeckmann, Brigitte and Bairoch, Amos and Apweiler, Rolf and Blatter,
	Marie-Claude and Estreicher, Anne and Gasteiger, Elisabeth and Martin,
	Maria J. and Michoud, Karine and O'Donovan, Claire and Phan, Isabelle
